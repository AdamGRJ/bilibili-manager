{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "clean": "rimraf node_modules package-lock.json"
  },
  "dependencies": {
    "axios": "1.8.1",
    "echarts": "5.6.0",
    "vue": "3.4.27",
    "vue-echarts": "^7.0.3",
    "vue-router": "4.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/eslint-parser": "7.19.1",
    "@eslint/js": "^9.3.0",
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "core-js": "^3.41.0",
    "eslint": "8.57.1",
    "eslint-plugin-vue": "8.7.1",
    "rimraf": "^5.0.5"
  },
  "resolutions": {
    "@vue/cli-service": "5.0.8",
    "eslint": "9.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true
    },
    "extends": [
      "plugin:vue/vue3-recommended",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "ecmaVersion": 2022,
      "sourceType": "module"
    },
    "rules": {
      "vue/multi-word-component-names": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
